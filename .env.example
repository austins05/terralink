# Tabula API Configuration
TABULA_API_URL=https://test-api.tracmap.com/v1
TABULA_API_TOKEN=your_api_token_here

# Server Configuration
PORT=3000
NODE_ENV=development

# CORS Configuration (Rotorsync iOS app)
ALLOWED_ORIGINS=*

# Order Monitoring
# Set to 'true' to auto-start order monitor on server startup
ENABLE_ORDER_MONITOR=false

# Email Notification Configuration
# Configure via API at /api/notifications/config or edit notification-config.json
# From address for outgoing emails
EMAIL_FROM="Terralink Notifications <noreply@terralink.local>"
